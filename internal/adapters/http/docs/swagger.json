                    {
                        "description": "Batch Budget Payload",
                        "name": "payload",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.SetBudgetBatchRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
